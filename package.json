{
  "name": "@gourmetpro/vue-stripe-js",
  "version": "0.2.0",
  "description": "Vue components for Stripe.js and Stripe Elements",
  "main": "dist/vue-stripe-js.ssr.js",
  "browser": "dist/vue-stripe-js.esm.js",
  "module": "dist/vue-stripe-js.esm.js",
  "unpkg": "dist/vue-stripe-js.min.js",
  "types": "dist/vue-stripe-js.min.d.ts",
  "files": [
    "dist",
    "src"
  ],
  "scripts": {
    "serve": "vue-cli-service serve dev/serve.ts",
    "build": "cross-env NODE_ENV=production rollup --config build/rollup.config.js",
    "build:ssr": "cross-env NODE_ENV=production rollup --config build/rollup.config.js --format cjs",
    "build:es": "cross-env NODE_ENV=production rollup --config build/rollup.config.js --format es",
    "build:unpkg": "cross-env NODE_ENV=production rollup --config build/rollup.config.js --format iife"
  },
  "keywords": [
    "Vue",
    "Stripe",
    "Elements"
  ],
  "author": "GourmetPro (https://www.gourmetpro.co)",
  "license": "MIT",
  "repository": {
    "type": "git",
    "url": "https://github.com/GourmetPro/vue-stripe-js.git"
  },
  "dependencies": {},
  "devDependencies": {
    "@babel/core": "^7.9.0",
    "@babel/preset-env": "^7.9.5",
    "@babel/preset-typescript": "^7.9.0",
    "@rollup/plugin-alias": "^2.2.0",
    "@rollup/plugin-commonjs": "^11.1.0",
    "@rollup/plugin-replace": "^2.3.2",
    "@stripe/stripe-js": "^1.5.0",
    "@vue/cli-plugin-babel": "^4.3.1",
    "@vue/cli-plugin-typescript": "^4.3.1",
    "@vue/cli-service": "^4.3.1",
    "@wessberg/rollup-plugin-ts": "^1.2.24",
    "cross-env": "^7.0.2",
    "minimist": "^1.2.5",
    "rollup": "^2.7.3",
    "rollup-plugin-babel": "^4.4.0",
    "rollup-plugin-node-resolve": "^5.2.0",
    "rollup-plugin-terser": "^5.3.0",
    "rollup-plugin-vue": "^5.1.6",
    "typescript": "^3.8.3",
    "vue": "^2.6.11",
    "vue-property-decorator": "^8.4.2",
    "vue-template-compiler": "^2.6.11"
  },
  "peerDependencies": {
    "vue": "^2.6.11"
  },
  "engines": {
    "node": ">=10"
  }
}
